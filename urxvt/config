! General
urxvt.title: (o.0)

! Disable all perl extension (If needed to be faster)
! urxvt.perl-ext:
! urxvt.perl-ext-common:

! All perl extensions
urxvt.perl-ext-common: default,matcher,tabbed

! Theme
urxvt.background: Black
urxvt.foreground: White
